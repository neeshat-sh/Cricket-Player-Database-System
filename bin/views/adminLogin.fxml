<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="570.0" prefWidth="740.0" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.AdminLoginController">
   <children>
      <ImageView fx:id="backgroundImageView" fitHeight="587.0" fitWidth="740.0" AnchorPane.bottomAnchor="-17.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <image>
            <Image url="@/views/images/background.jpg" />
         </image>
      </ImageView>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="202.0" layoutY="117.0" prefHeight="50.0" prefWidth="335.0" text="ADMIN LOGIN PAGE" textFill="#d2d4a7">
         <font>
            <Font name="Bank Gothic Medium BT" size="28.0" />
         </font></Label>
      <TextField fx:id="userText" alignment="CENTER" layoutX="330.0" layoutY="227.0" prefHeight="40.0" prefWidth="287.0">
         <font>
            <Font name="Monospaced Bold" size="18.0" />
         </font></TextField>
      <PasswordField fx:id="passwordText" alignment="CENTER" layoutX="330.0" layoutY="315.0" prefHeight="40.0" prefWidth="287.0">
         <font>
            <Font name="Monospaced Bold" size="18.0" />
         </font>
      </PasswordField>
      <Button fx:id="resetButton" alignment="CENTER" contentDisplay="CENTER" layoutX="474.0" layoutY="445.0" mnemonicParsing="false" onAction="#resetAction" text="Reset" textFill="#d15929">
         <font>
            <Font name="Monospace 821 Bold BT" size="16.0" />
         </font>
      </Button>
      <Button fx:id="loginButton" alignment="CENTER" contentDisplay="CENTER" layoutX="552.0" layoutY="445.0" mnemonicParsing="false" onAction="#handleLogin" text="Login" textFill="#d15929">
         <font>
            <Font name="Monospace 821 Bold BT" size="16.0" />
         </font>
      </Button>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="114.0" layoutY="235.0" text="Enter Username : " textFill="#d2d4a7">
         <font>
            <Font name="Monospace 821 Bold BT" size="18.0" />
         </font>
      </Label>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="113.0" layoutY="318.0" prefHeight="34.0" prefWidth="185.0" text="Enter Password : " textFill="#d2d4a7">
         <font>
            <Font name="Monospace 821 Bold BT" size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
